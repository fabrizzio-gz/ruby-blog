<%= alert %>

<h1>Log in</h1>

<%= form_with model: @user, url: create_post_path do |form| %>
  <div>
    <%= form.label :email, "Email:" %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label :password, "Password:" %>
    <%= form.password_field :password %>
  </div>

  <%= form.submit "Log in" %>
<% end %>
